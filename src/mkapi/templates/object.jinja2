<p class="mkapi-object" id="{{ id }}" markdown="1">
<span class="mkapi-object-link">[{{ namespace }}][__mkapi__.__{{ namespace }}__.{{ obj_id }}]</span>
{%- if kind not in ("function") -%}
<span class="mkapi-object-kind">{{ kind }}</span>
{%- endif %}
<span class="mkapi-object-name">[{{ name }}][__mkapi__.{{ obj_id }}]</span>
{%- if signature -%}
<span class="mkapi-signature">
{%- for name, kind in signature -%}
<span class="mkapi-{{ kind }}">{{ name|safe }}</span>
{%- endfor -%}
</span>
{%- endif %}
</p>
{%- if bases -%}
<p class="mkapi-object-bases" markdown="1">Bases :
{% for base in bases %}<span class="mkapi-object-base">{{ base|safe }}</span>
{%- if not loop.last %}<span class="mkapi-comma">, </span>{% endif -%}
{%- endfor -%}
</p>
{%- endif %}
